{
  "name": "dashsight-server",
  "version": "1.0.0",
  "description": "Safely exposes Dash RPCs and MasterNode TCP Relay to Web & Mobile Wallets.",
  "main": "dashsight-server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "jshint": "npx -p jshint@2.x -- jshint -c ./.jshintrc ./*.js",
    "lint": "npm run jshint && npm run tsc",
    "prettier": "npx -p prettier@2.x -- prettier -w '**/*.{js,md}'",
    "fmt": "npm run prettier",
    "bump": "npm version -m \"chore(release): bump to v%s\"",
    "tsc": "npx -p typescript@4.x -- tsc -p ./jsconfig.json",
    "reexport-types": "npx -p jswt@1.x -- reexport",
    "prepublish": "npm run reexport-types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dashhive/DashSight-Server.js.git"
  },
  "keywords": [
    "dash",
    "rpc",
    "relay"
  ],
  "author": "AJ ONeal <https://therootcompany.com> (aj@therootcompany.com)",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/dashhive/DashSight-Server.js/issues"
  },
  "homepage": "https://github.com/dashhive/DashSight-Server.js#readme",
  "dependencies": {
    "@root/async-router": "^1.0.14",
    "dashrpc": "^19.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "knuth-shuffle": "^1.0.8",
    "morgan": "^1.10.0",
    "ws": "^8.17.0"
  }
}
